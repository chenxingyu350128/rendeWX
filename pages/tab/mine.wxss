/* pages/tab/mine.wxss */
.personal{
  display: flex;
  flex-direction: row;
  margin-top: 30rpx;
}
.headSculptureImg{
  width: 124rpx;
  height: 124rpx;
  border-radius: 100%;
  border: 2rpx solid  #479eff;
  margin-left: 40rpx;
}
.name{
  font-size: 0.85em;
  font-weight: bold;
  margin-top: 10rpx;
}
.tel{
  font-size: 0.75em;
  margin-left: 10rpx;
}
.personal-info{
  margin-left: 50rpx;
  display: flex;
  flex-direction: column;
}
.tel-wrap{
  display: inline-block;
}
.tel-wrap text{
  font-size: 22rpx;
}
.tel-wrap .recommand{
  margin-left: 108rpx;
}
.telImg{
  height: 20rpx;
  width: 14rpx;
  margin-right: 10rpx;
}
.enterImg{
  width: 29rpx;
  height: 23rpx;
  margin-right: 10rpx;
}
.btn-wrap1-wrap{
  display: flex;
  justify-content: center;
}
.btn-wrap1{
  display: flex;
  flex-direction: row;
  width: 100%;
  margin-top: 56rpx;
  padding-bottom: 39rpx;
}
.line-wrap{
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.line{
  width: 90%;
  border-bottom: 2rpx solid #cccccc;
}
.btn-wrap1-item{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 25%;
}
.btn-wrap1-item image{
  width: 72rpx;
  height: 58rpx;
}
.btn-wrap1 text{
  font-size: 0.65em;
  margin-top: 22rpx;
}
.mid{
  border-radius: 10rpx;
}
.mid-wrap{
  /* display: flex;
  justify-content: center; */
  margin-top: 38rpx;
  position: relative;
  padding: 0 30rpx;
  height: 282rpx;  
}
.mid-wrap>image{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
  box-shadow: 0 7rpx 20rpx #979797;
}
.inner-img{
  position: absolute;
  bottom:16rpx;
}


.resume{
  margin-top: 41rpx;
  margin-left: 59rpx;
  font-size: 1em;
  color: #fff;
  font-weight: bold;
}
.lookResume-wrap{
  display: flex;
  flex-direction: row;
  /* justify-content: center; */
  align-items: center;
  margin-top: 33rpx;
}

.lookResume{
  margin-left: 59rpx;
  margin-right: 16rpx;
  font-size: 0.8em;
  color: #fff;
  font-weight: bold;
}
.arrow{
  width: 15rpx;
  height: 15rpx;
  border:#fff solid;
  border-width: 4rpx 4rpx 0 0;
  transform: rotate(45deg);
}
.percent-text{
  margin-left: 59rpx;
}
.percent-wrap{
  font-size: 0.65em;
  color: #fff;
}
.bottom{
  display: flex;
  flex-direction: column;
  margin-top: 39rpx;
}

.bottom-title{
  padding-left: 14rpx;
  border-left: 4rpx solid #3385ff;
  margin-left: 30rpx;
  font-weight: bold;
  font-size: 0.8em;
}
.btn-wrap2{
  display: flex;
  flex-direction: row;
  margin-top: 46rpx;
  padding-left: 66rpx;
}
.btn-wrap2-item{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-right: 85rpx;
  width: 100rpx;
  height: 92rpx;
}
.btn-wrap2-item text{
  font-size: 0.6em;
  margin-top: 17rpx;
}
.bottom-wrap .bottom:nth-child(1) .btn-wrap2 .btn-wrap2-item:nth-child(1) image{
  width: 45rpx;
  height: 49rpx;
}
.bottom-wrap .bottom:nth-child(1) .btn-wrap2 .btn-wrap2-item:nth-child(2) image{
  width: 46rpx;
  height: 50rpx;
}
.bottom-wrap .bottom:nth-child(2){
  margin-top: 66rpx;
  padding-bottom: 36rpx;
}
.bottom-wrap .bottom:nth-child(1) .btn-wrap2 .btn-wrap2-item:nth-child(3) image{
  width: 47rpx;
  height: 47rpx;
}
.bottom-wrap .bottom:nth-child(2) .btn-wrap2 .btn-wrap2-item:nth-child(1) image{
  width: 59rpx;
  height: 36rpx;
}
.bottom-wrap .bottom:nth-child(2) .btn-wrap2 .btn-wrap2-item:nth-child(2) image{
  width: 38rpx;
  height: 44rpx;
}
.bottom-wrap .bottom:nth-child(2) .btn-wrap2 .btn-wrap2-item:nth-child(3) image{
  width: 42rpx;
  height: 42rpx;
}